{% extends 'base.html.twig' %}

{% block body %}
<div class="container">
	<div class="panel panel-primary">
		<div class="panel-heading">
			<h3 class="panel-title main-title">Formulario de Inscripción Organización Pública</h3>
		</div>
		<div class="panel-body">
			<div class="row">
				<div class="col-sm-12">
			    {% include ':organizacionpublica:form.html.twig' %}
				</div>
			</div>
		</div>
	</div>
	{% if is_granted('ROLE_ADMIN') %}
    <ul class="list-inline">
        <li>
            <a class="btn btn-default" href="{{ path('organizacionpublica_index') }}">
            	<i class="fa fa-chevron-left"></i> Volver a la Lista
            </a>
        </li>
    </ul>
	{% endif %}
</div>
{% endblock %}

{% block page_javascripts %}
	{% include ':organizacionpublica:form-javascripts.html.twig' %}
{% endblock %}